{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "A React boilerplate",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-server && npm run build-client",
    "build-server": "babel src/server --out-dir ./dist",
    "build-client": "babel src/client --copy-files --out-dir ./dist/public",
    "start": "node ./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajdelossantos/react-boilerplate.git"
  },
  "keywords": [
    "Node",
    "React"
  ],
  "author": "Alvin James T. Delos Santos",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/ajdelossantos/react-boilerplate/issues"
  },
  "homepage": "https://github.com/ajdelossantos/react-boilerplate#readme",
  "dependencies": {
    "eslint": "^4.19.1",
    "express": "^4.16.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0"
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}